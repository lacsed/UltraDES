<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>2.1.2</Version>
    <Authors>Developers of LACSED</Authors>
    <Company>LACSED | UFMG</Company>
    <Description>UltraDES is a library for modeling, analysis and control of Discrete Event Systems.</Description>
    <PackageLicenseExpression></PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/lacsed/UltraDES</PackageProjectUrl>
    <PackageIconUrl>http://lacsed.eng.ufmg.br/wp-content/uploads/2017/05/Logo_UltraDES_PNG_Internet-e1494353854950.png</PackageIconUrl>
    <PackageTags>linqpad-samples</PackageTags>
    <NeutralLanguage>en</NeutralLanguage>
    <PackageReleaseNotes>- Implemented an Implicit conversion from ValueTuple to Transition
- Minor performace improvementes</PackageReleaseNotes>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="..\linqpad-samples\**">
      <Pack>true</Pack>
      <PackagePath>\linqpad-samples\</PackagePath>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>
  
</Project>
